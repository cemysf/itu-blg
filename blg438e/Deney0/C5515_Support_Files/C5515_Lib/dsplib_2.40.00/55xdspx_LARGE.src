	MOV	AC0, T3			;T3 = x
	||MOV	*ar_coeff+ <<#16, AC0	;AC0(high bits) = c5
	MOV	*ar_coeff+ <<#16, AC3	;AC3(high bits) = c4

	||BTST	#15, *ar_x+, TC1

	MOV	*ar_coeff+ <<#16, AC3
	||MACR AC0, T3, AC3, AC0

	MOV	*ar_coeff+ <<#16, AC3
	||MACR AC0, T3, AC3, AC0

	MOV	*ar_coeff+ <<#16, AC3
	||MACR AC0, T3, AC3, AC0

