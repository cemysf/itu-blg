            AADD        #2,SP                  ; destroy local frame
            POP         mmap(ST3_55)
            POP         mmap(ST2_55)
            POP         mmap(ST1_55)
            POP         mmap(ST0_55)

;//-----------------------------------------------------------------------------
;// Return
;//-----------------------------------------------------------------------------
            RET

        .end

