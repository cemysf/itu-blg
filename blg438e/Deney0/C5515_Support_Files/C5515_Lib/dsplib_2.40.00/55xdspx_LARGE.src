	CALL	atan2
	||MANT	AC0, AC1
	::NEXP	AC0, T1
	SUB	#16384 << #16, AC0
	NEG	AC0
	||B	QUAD
XGTY
	MOV	HI(AC1), *SP(#SP_NUM)
	CALL	atan2
	||MANT	AC0, AC1
	::NEXP	AC0, T1

QUAD
