	MACR	AC0, T0, AC1, AC0
	MPYR	T0, AC0
	MOV	HI(AC0), *AR1+		;save result

loop1:

	BCLR	FRCT			;return to standard C
	
	MOV	#0, T0			;return OK value (no possible error)
	
	||RET

