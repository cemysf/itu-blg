;****************************************************************
	.mmregs
	.cpl_on
	.arms_on
	;.model call=c55_std
	;.model mem=small

	.def	_atan16
;FRCT	.set	6			; FRCT bit position in ST1 (phase3)

_atan16:
;* AR0   assigned to _x
;* AR1   assigned to _r
;* T0   assigned to _nx
	SUB	#1, T0			;nx-1
	MOV	T0, BRC0		;repeat nx times
	AMOV	#atancoeffs, XAR2	;pointer on coeff
	BSET	FRCT			;fractional mode
	RPTBLOCAL	loop1-1
	MOV	*AR2+ << #16, AC0	;C5
