	MOV	HI(AC1 << #0), *SP(#11)
	MOV40	dbl(*SP(#0)), AC1
	AND	AC1, AC0
	BCC	zero, AC0 == 0
	AND	#32640 << #16, AC0, AC1
	MOV	*(#(12)) << #-7, AC1
	SUB	*SP(#5), AC1

;----------------------------------------------------------------
; If unbiased exponent is >= 0, then value is too large to
; represent in Q15 format
;----------------------------------------------------------------

	BCC	too_large, AC1 >= #0

;----------------------------------------------------------------
; If exponent (abs) exp > abs(min_exp) then value is too small
; to represent in Q15 format. (min_exp = -15, since 2^-15 is
; least significant bit of Q15 number
;----------------------------------------------------------------

	MOV	AC1 << #0, *SP(#3)
	ABS	AC1
	SUB	*SP(#6), AC1
	
	BCC	too_small, AC1 > #0

;----------------------------------------------------------------
; Shift floating point mantissa to keep only the most significant
; 15 bits. The implied 1 of the IEEE floating point format is ORed
; in as most significant bit.
;----------------------------------------------------------------

	MOV	*SP(#11), AC1
	SFTSC	AC0, #-7
	MOV	*(#(8)) << #-1, AC0
	AND	#65504, mmap(ST1)
	MOV	*SP(#3), T2
	OR	*SP(#7), AC0
	SFTS	AC0, T2
	XCC	check1, AC1 != #0
	NEG	AC0
check1:

P4_9:
	B	store_result

too_small:
	MOV	#2, *SP(#10)

zero:
	MOV	#0, AC0
	B	store_result

too_large:
	MOV	*SP(#11), AC1
	MOV	#1, *SP(#4)
	MOV	*SP(#9), AC0
	XCC	check2, AC1 > #0
	ADD	*SP(#8), AC0
check2:

store_result:
	MOV	AC0 << #0, *AR1+
	MOV40	dbl(*AR0+), AC0

end_loop:

;----------------------------------------------------------------
; Return to calling program. Set error conditions on return"
;----------------------------------------------------------------"

Epilogue:

	MOV	*SP(#4), T0
	OR	*SP(#10), T0

	AADD	#13, SP

	POP     T2
; ZHENGTING ADDED TO RESTORE T2

	BSET	SXMD
	RET
	



