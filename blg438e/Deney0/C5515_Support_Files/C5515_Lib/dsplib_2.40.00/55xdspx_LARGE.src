	MOV	AC2, T0
	||MOV	*AR3+ << #16, AC1
	MOV	*AR3+ << #16, AC1
	||MACR	AC0, T0, AC1, AC0
	MOV	*AR3+ << #16, AC1
	||MACR	AC0, T0, AC1, AC0
	MOV	*AR3+ << #16, AC1
	||MACR	AC0, T0, AC1, AC0
	MACR	AC0, T0, AC1, AC0
	MPYR	T0, AC0

	SFTS	AC0, #-1
	||RET

